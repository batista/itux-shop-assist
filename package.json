{
  "name": "@batista/itux-shop-assist",
  "version": "1.1.1",
  "description": "Your shop assistant that helps you to select the next internet provider from either itux.se or framtidensbredband.se",
  "main": "lib/main.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --minify --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test && npm run package",
    "start": "npm run build && node lib/main.js",
    "help": "npm run build && node lib/main.js --help",
    "npx:local": "npm run all && npm install -g && npx . && npm uninstall -g",
    "publish": "npm run all && npx np"
  },
  "author": {
    "name": "Sergio Batista",
    "email": "dev@se.rg.io"
  },
  "files": [
    "dist/**/*",
    "assets/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batista/itux-shop-assist.git"
  },
  "keywords": [
    "bredband",
    "Framtidens Bredband",
    "Framtidens",
    "itux"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.7.15",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/parser": "^5.9.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.6.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.4",
    "jest": "^27.4.7",
    "jest-ts-auto-mock": "^2.0.0",
    "prettier": "2.5.1",
    "ts-auto-mock": "^3.5.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "16.13.1"
  }
}
